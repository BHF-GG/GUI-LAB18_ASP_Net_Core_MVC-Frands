<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - LAB18_ASP_Net_Core_MVC</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body>
<div id="wrapper">
    <header>
        <h1>
            <img src="images/javalogo.gif" alt="JavaJam Coffee House" height="117" width="619">
        </h1>
        
    </header>
    <nav>
        <ul>
            <li><a asp-controller="Home" asp-action="Index">Index</a></li>
            <li><a asp-controller="Home" asp-action="Menu">Menu</a></li>
            <li><a asp-controller="Home" asp-action="Music">Music</a></li>
        </ul>
    </nav>
    <div id="content">
        <main> @RenderBody()
        </main>
        <hr />
        <footer>
            Copyright &copy; 2018 JavaJam Coffee House<br />
            <a href="mailto:JavaJam@nowhere.com">JavaJam@nowhere.com</a>
        </footer>
    </div>
</div> @RenderSection("Scripts", required: false)
</body>
</html>
